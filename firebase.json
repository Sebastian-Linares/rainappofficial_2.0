{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    },
    "rewrites": [
      {
        "source": "**",
        "function": "nextjs"
      }
    ],
    "cleanUrls": true
  },
  "functions": {
    "source": "functions",
    "runtime": "nodejs18"
  }
}
